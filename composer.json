{
    "name": "fabiang/doctrine-migrations-liquibase-laminas",
    "description": "Laminas module for creating changesets for Liquibase with Doctrine",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "github@lubyte.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~8.3.0 || ~8.4.0",
        "doctrine/dbal": "^3.10 || ^4.0",
        "doctrine/doctrine-module": "^6.3",
        "doctrine/orm": "^2.0 || ^3.0",
        "fabiang/doctrine-migrations-liquibase": "^3.0",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-servicemanager": "^3.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^3.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "^3.13",
        "symfony/cache": "^5.4"
    },
    "replace": {
        "fabiang/doctrine-migrations-liquibase-zf": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Fabiang\\DoctrineMigrationsLiquibase\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "module": "Fabiang\\DoctrineMigrationsLiquibase"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "lint": [
            "@phpcs"
        ],
        "test": [
            "@lint",
            "@unittests"
        ],
        "phpcs": "phpcs --colors",
        "unittests": "phpunit --coverage-text --colors"
    }
}
